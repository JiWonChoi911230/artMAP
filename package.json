{
  "name": "artin-dna",
  "version": "0.1.2",
  "description": "Application for parsing dna",
  "bin": {
    "artin-dna": "lib/server/app.js"
  },
  "scripts": {
    "start": "node lib/server/app.js",
    "dev": "source activate my_root && DEBUG=dna:* NODE_ENV=dev node lib/server/app.js",
    "prepublish": "npm run clean && npm run build",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "babel src/server -d lib/server --source-maps",
    "build:client": "webpack --config webpack.config.prod.js",
    "clean": "rimraf lib",
    "lint": "eslint src/**"
  },
  "main": "dist/app.js",
  "preferGlobal": true,
  "author": "Peter Javorka <peter.javorka@artin.cz",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "commander": "^2.13.0",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.29",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.2",
    "shelljs": "^0.8.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "tmp": "0.0.33",
    "whatwg-fetch": "^2.0.3"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
