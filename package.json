{
  "name": "dna-v2",
  "version": "0.1.0",
  "description": "application for parsing dna",
  "bin": {
    "artin-dna": "lib/app.js"
  },
  "scripts": {
    "start": "DEBUG=dna:* node lib/server/app.js",
    "dev": "DEBUG=dna:* NODE_ENV=dev node lib/server/app.js",
    "test": "jest",
    "prepublish": "npm run clean && npm run build && npm run test",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "babel src/server -d lib/server --source-maps",
    "build:client": "npm run build:css && npm run build:webpack",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build:css": "node-sass src/client/style/sass/style.scss -o lib/server/public",
    "clean": "rimraf lib",
    "lint": "eslint src/**"
  },
  "main": "dist/app.js",
  "preferGlobal": true,
  "author": "Peter Javorka <peter.javorka@artin.cz",
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "chart.js": "^2.7.1",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-chartjs": "^0.8.0",
    "react-dom": "^16.1.1",
    "react-modal": "^3.1.6",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "tmp": "0.0.33",
    "validator": "^9.1.2"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
